{
  "name": "jsonapi-normalizer",
  "version": "0.1.0",
  "description": "Normalizes JSONAPI responses for use in Redux or similar systems.",
  "main": "dist/index.js",
  "scripts": {
    "build": "browserify src/index.js -o dist/index.js -t [ babelify ]",
    "test": "npm run lint && jest",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "stevenpetryk/jsonapi-normalizer"
  },
  "keywords": [
    "jsonapi",
    "json",
    "normalizr",
    "redux"
  ],
  "author": "Steven Petryk <me@stevenpetryk.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenpetryk/jsonapi-normalizer/issues"
  },
  "homepage": "https://github.com/stevenpetryk/jsonapi-normalizer#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-jest": "^16.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "jest": "^16.0.1",
    "standard": "^8.5.0"
  },
  "standard": {
    "globals": [
      "afterEach",
      "beforeEach",
      "afterAll",
      "beforeAll",
      "describe",
      "expect",
      "it",
